interval=10

[volume]
command=~/.config/i3blocks/scripts/volume.py
interval=10

[brightness]
command=~/.config/i3blocks/scripts/brightness.py
interval=10

[cpu]
command=~/.config/i3blocks/scripts/cpu.py
interval=5

[memory]
command=~/.config/i3blocks/scripts/memory.py
interval=5

[battery]
command=~/.config/i3blocks/scripts/battery.py
interval=30

[dectime]
command=~/.config/i3blocks/scripts/dectime.py
interval=1


[datetime]
command=echo "DAT $(date '+%Y/%m/%d') TIM $(date '+%H:%M:%S')"
interval=1

